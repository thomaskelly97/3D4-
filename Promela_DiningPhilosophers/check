echo "ANALYSIS STARTS"
rm *.trail

echo "SIMULATE 100 STEPS"
spin -u100 dphil.pml
echo ""

echo "COMPILE VERIFIER"
cc -o pan pan.c 
./pan

echo ""

echo "STANDARD VERIFICATION"
cc -DREACH -o pan pan.c 
./pan
echo ""

echo "PROGRESS VERIFICATION"
cc -DNP -o pan pan.c 
./pan

echo ""
echo "ANALYSIS COMPLETE"
